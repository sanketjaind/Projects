                    google_ad_width = ww - wa;
                }

                // T16060 AsSense limitations:
                var max_width  = 1200; // Width cannot exceed 1200 pixels
                var max_height = 1200; // Height cannot exceed 1200 pixels
                var min_width  = 120;  // The minimum width is 120 pixels
                var min_height = 50;   // The minimum height is 50 pixels
                var max_width_and_height = 300; // Only one dimension can be greater than 300 pixels

                if (google_ad_width > max_width 
                    || google_ad_height > max_height
                    || google_ad_width  < min_width
                    || google_ad_height < min_height
                    || (google_ad_width > max_width_and_height && google_ad_height > max_width_and_height)) {

                    google_ad_width  = Math.min(max_width,  Math.max(min_width,  google_ad_width));
                    google_ad_height = Math.min(max_height, Math.max(min_height, google_ad_height));

                    if (google_ad_height > max_width_and_height && google_ad_width > max_width_and_height) {
                        // enforcement not yet needed, leaving note here if later textad development has a dynamic height
                    }
                }
            })();
